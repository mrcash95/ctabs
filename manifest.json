{
  "manifest_version": 2,
  "name": "CTabs",
  "description": "This extension is a prototype for combatting Tabnabbing attack, enchanced from TabShots Extension",
  "version": "1.0",
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
  "background": {
  		"page": "ctabs.html" 
                },
  "browser_action": {
    "default_icon": "images/16.png",
    "default_title": "CTabs",
    "default_popup": "popup.html"
                    },
    "icons" : {
        "64" : "images/64.png"
    },
  "permissions": [
        "tabs",
  		"<all_urls>",
  		"storage",
      //all below are from Blacklist
        "webRequest",
        "webRequestBlocking",
        "webNavigation"
                 ]
}
